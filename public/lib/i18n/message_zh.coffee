    year: (y)->
        y = y.toString()
        str = ''
        for n in y
            str += '零一二三四五六七八九十'[parseInt(n)]
        str + '年'

    time: (hour)->
        if hour > 12 then hour - 12 + ':00<span>下午</span>' else hour + ':00<span>上午</span>'

    mDate: (m,d)->
        m+'月'+d + '日'
    date: (d,m,y)->
        str = ''
        str += y + '年' if y
        str +=( m+1) + '月' if m isnt null
        str += d + '日' if d
        str

    time_sec: (s, e, st, et)->
        "#{s}至#{e}#{if st and et then '<br/>每天' + st + '到' + et else ''}"
