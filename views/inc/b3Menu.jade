ul(class= mClass || 'nav navbar-nav')
    if menu
        each it in menu
            li(class= it.code || '')
                a(href='#{it.href}') #{it.label}
                if _showSub && it.children
                    ul.dropdown-menu
                        each sub in it.children
                            li
                                a(href='#{sub.href}') #{sub.label}